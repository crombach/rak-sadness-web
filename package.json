{
  "name": "rak-sadness-web",
  "productName": "Rak Sadness",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Cullen Rombach",
    "email": "cullenrombach@gmail.com"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-10-30 --port 3000 -- npm start",
    "pages:deploy": "npm run build && wrangler pages deploy ./build",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.15",
    "@mui/icons-material": "^5.14.16",
    "@mui/joy": "^5.0.0-beta.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "typescript": "^4.9.5",
    "wrangler": "^3.15.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
